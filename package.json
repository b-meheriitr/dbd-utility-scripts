{
  "name": "utility-scripts",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "files": [
    "dist",
    "package.json"
  ],
  "bin": {
    "scripts-bundle": "dist/lib/bundle.js"
  },
  "scripts": {
    "lint": "eslint -c lint/.eslintrc.js \"{src,test}/**/*.{js,ts}\"",
    "lint:fix": "eslint -c lint/.eslintrc.js \"{src,test}/**/*{.js,ts}\" --fix",
    "lint-staged": "lint-staged --no-stash",
    "test": "ts-node src/lib/node/index.js",
    "build": "ts-node scripts/clean-dist.js && tsc"
  },
  "keywords": [
    "utility",
    "scripts",
    "deployment"
  ],
  "author": "Bhubaneshwar.Meher@ril.com",
  "license": "ISC",
  "dependencies": {
    "esbuild": "^0.18.11",
    "lodash": "^4.17.21",
    "minimist": "^1.2.8"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "eslint": "^8.44.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-no-useless-assign": "^1.0.3",
    "eslint-plugin-unused-imports": "^2.0.0",
    "lint-staged": "^13.2.3",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "lint-staged": {
    "{src,test}/**/*.{js,jsx,ts,css,html}": [
      "eslint -c lint/.eslintrc.js --fix"
    ],
    "*": []
  }
}
