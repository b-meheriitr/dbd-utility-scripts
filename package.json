{
  "name": "utility-scripts",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "files": [
    "dist",
    "package.json"
  ],
  "directories": {
    "bin": "dist/bin"
  },
  "scripts": {
    "lint": "eslint -c lint/.eslintrc.js \"{src,test}/**/*.{js,ts}\"",
    "lint:fix": "eslint -c lint/.eslintrc.js \"{src,test}/**/*{.js,ts}\" --fix",
    "lint-staged": "lint-staged --no-stash",
    "test-bundle": "ts-node src/bin/bundle.js",
    "test-clean": "ts-node src/bin/clean.js",
    "test-clean-bundle": "ts-node src/bin/clean-bundle.js",
    "test-deploy": "ts-node src/bin/deploy.js --env=local",
    "test-buildoh": "ts-node src/bin/buildoh.ts",
    "build": "ts-node scripts/clean-dist.js && tsc && ts-node scripts/chmod_x_bins.js && ts-node scripts/rename-bins.js"
  },
  "keywords": [
    "utility",
    "scripts",
    "deployment"
  ],
  "author": "Bhubaneshwar.Meher@ril.com",
  "license": "ISC",
  "dependencies": {
    "archiver": "^5.3.1",
    "axios": "^1.4.0",
    "esbuild": "^0.18.11",
    "form-data": "^4.0.0",
    "glob": "^10.3.3",
    "ignore": "^5.2.4",
    "lodash": "^4.17.21",
    "minimist": "^1.2.8",
    "moment": "^2.29.4",
    "rimraf": "^5.0.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "eslint": "^8.44.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-no-useless-assign": "^1.0.3",
    "eslint-plugin-unused-imports": "^2.0.0",
    "lint-staged": "^13.2.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "lint-staged": {
    "{src,test}/**/*.{js,jsx,ts,css,html}": [
      "eslint -c lint/.eslintrc.js --fix"
    ],
    "*": []
  }
}
