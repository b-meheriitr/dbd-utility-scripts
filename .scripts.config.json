{
  "profileTime": true,
  "appName": "test-zz",
  "deployment": {
    "dev": {
      "api": {
        "baseUrl": "http://localhost:8011"
      },
      "copyFiles": [
        "*.json",
        {
          "cwd": ".",
          "pattern": ".out/**"
        }
      ]
    },
    "artifactZipUrlConfig": {
      "headers": {
        "app-name": "test-zz"
      }
    }
  },
  "build": {
    "buildInfo": {
      "usesDbdBuilder": true,
      "bundle": {
        "bundledDependencies": "*",
        "esbuildConfig": {
          "entryPoints": [
            "test/index.js"
          ],
          "minify": true,
          "bundle": true,
          "metafile": false
        }
      },
      "commands": [
        "npm i --include=dev",
        {
          "env": "dev",
          "command": "ts-node src/bin/build.js <cliArgs>"
        },
        {
          "env": "test",
          "command": "npm -v -- <cliArgs>"
        }
      ]
    },
    "buildoh": {
      "codeBaseZipIgnore": [
        "node_modules/**"
      ],
      "copyFiles": [
        "*.json",
        {
          "cwd": ".",
          "pattern": ".out/**"
        }
      ],
      "api": {
        "baseUrl": "http://localhost:8011"
      },
      "zipDownloadTo": ".zip3332"
    },
    "buildPath": "./dist/lib",
    "dependencyPackagesFilePatterns": [
      "node_modules/**"
    ],
    "cleanBuildIgnoreDelete": [],
    "copyFiles": [
      "*.json",
      {
        "cwd": ".",
        "pattern": [
          "test/**/*"
        ]
      }
    ]
  }
}
