{
  "profileTime": true,
  "appName": "test-zz",
  "deployment": {
    "dev": {
      "api": {
        "baseUrl": "http://localhost:8011"
      }
    }
  },
  "build": {
    "buildInfo": {
      "usesDbdBuilder": true,
      "bundle": {
        "bundledDependencies": "*",
        "esbuildConfig": {
          "entryPoints": [
            "test/index.js"
          ],
          "minify": true,
          "bundle": true,
          "metafile": false
        }
      }
    },
    "buildoh": {
      "codeBaseZipIgnore": [
        "node_modules/**"
      ],
      "api": {
        "baseUrl": "http://localhost:8011"
      }
    },
    "buildPath": "./dist/lib",
    "dependencyPackagesFilePatterns": [
      "node_modules/**"
    ],
    "cleanBuildIgnoreDelete": [],
    "copyFiles": [
      {
        "cwd": ".",
        "pattern": [
          "test/**/*"
        ]
      }
    ]
  }
}
